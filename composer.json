{
    "name": "avia-avian/laravel-dremio-odbc",
    "description": "Laravel ODBC driver for Dremio (Arrow Flight SQL ODBC).",
    "keywords": ["laravel", "odbc", "dremio", "database"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Software Engineer Team",
            "email": "it_7@avianbrands.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "AviaAvian\\DremioOdbc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AviaAvian\\DremioOdbc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AviaAvian\\DremioOdbc\\Providers\\OdbcServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
